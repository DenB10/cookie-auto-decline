{
   "manifest_version":3,
   "name":"Cookie auto decline",
   "author":"JobcenterTycoon",
   "version":"6.0",
   "description":"Die Erweiterung akzeptiert Cookie Banner automatisch mit minimaler Zustimmung.",
   "homepage_url":"https://github.com/JobcenterTycoon/cookie-auto-decline",
   "icons":{
      "128":"icons/icon_128.png",
      "64":"icons/icon_64.png",
      "32":"icons/icon_32.png",
      "16":"icons/icon_16.png"
   },
   "browser_specific_settings":{
      "gecko":{
         "id":"jobcentertycoon@gmail.com",
         "strict_min_version":"109.0"
      },
      "gecko_android":{
         "strict_min_version":"113.0"
      }
   },
   "content_scripts":[
      {
         "matches":[
            "*://*/*"
         ],
         "exclude_matches":[
            "*://127.0.0.1/*", "*://0.0.0.0/*", "*://[::]/*", "*://[::1]/*", "*://localhost/*", "*://*.local/*", "*://*.internal/*", "*://fritz.box/*", "*://fritz.nas/*", "*://fritz.repeater/*", "*://myfritz.box/*", "*://giga.cube/*", "*://congstar.box/*", "*://easy.box/*", "*://kabel.box/*", "*://pi.hole/*", "*://speedport.ip/*", "*://my.router/*", "*://home.arpa/*"
         ],
         "js":[
            "script.js"
         ],
         "all_frames":true,
         "run_at":"document_idle"
      },
      {
         "matches":[
            "*://*/*"
         ],
         "exclude_matches":[
            "*://127.0.0.1/*", "*://0.0.0.0/*", "*://[::]/*", "*://[::1]/*", "*://localhost/*", "*://*.local/*", "*://*.internal/*", "*://fritz.box/*", "*://fritz.nas/*", "*://fritz.repeater/*", "*://myfritz.box/*", "*://giga.cube/*", "*://congstar.box/*", "*://easy.box/*", "*://kabel.box/*", "*://pi.hole/*", "*://speedport.ip/*", "*://my.router/*", "*://home.arpa/*"
         ],
         "js":[
            "setterscript.js"
         ],
         "all_frames":true,
         "run_at":"document_start"
      }
   ],
   "host_permissions":[
      "<all_urls>"
   ],
   "permissions":[
      "activeTab",
      "storage"
   ],
   "action":{
      "default_icon":{
         "16":"icons/icon_16.png",
         "32":"icons/icon_32.png",
         "48":"icons/icon_64.png",
         "128":"icons/icon_128.png"
      },
      "default_title":"Cookie auto decline",
      "default_popup":"popup/popup.html"
   }
}
